{
    
        "background": {
            "scripts": [ "background.js" ]
        
    },
    "manifest_version": 2,
    "minimum_chrome_version": "31",
    "name": "Shopify Product Scraper",
   
    "icons": {
        "128": "icon.png"
    },
 
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png",
    "default_title": "Shopify Product Scraper"
  },
    "version": "2.1",
"content_scripts": [ 
    {
      "js": ["/js/content/main.js","/js/content/content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
  } ]
}
